<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.ktb.dao.EmployeeDao">
    <select id="findOne" resultType="com.ktb.entity.Employee">
        SELECT  `tbl_employee`.* FROM `tbl_employee` WHERE `tbl_employee`.`id` = #{id}
    </select>
</mapper>